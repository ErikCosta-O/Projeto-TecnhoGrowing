                        <div class="dadosONGredes">
                            <ul>
                                <a href="#"><li>Facebook</li></a>
                            </ul>
                    </div>
                </div>
            </div>
        </section>
        <section id="descVaga">
            <div class="nomeVaga"><p>Nome da Vaga</p></div>
            <div class="descConteudoVaga"><p><!--o formato é assim e o texto pode ficar em totalmente em uma linha-->
                
            </p>
            </div>
            <div class="descVaga2">
                <label class="lbldesc2">Horários e Endereço da vaga</label>
                <ul>
                    <li><img src="../images/calendario.png"><p></p></li>
                    <li><img src="../images/relogio-nove.png"><p></p></li>
